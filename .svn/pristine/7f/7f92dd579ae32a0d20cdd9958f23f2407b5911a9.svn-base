package com.lgcns.laf.collection;

import java.util.HashMap;
import java.util.Map;

public class LBox {
	private String name = "";
	
	private Map<String, String> datas = new HashMap<String, String>();
	
	public LBox(String name) {
		this.name = name;
	}
	
	public void put(String key, String value) {
		datas.put(key, value);
	}
}